    <![endif]-->        
      <link rel="stylesheet" type="text/css" href="css/bootstrap.css">
      <link rel="stylesheet" type="text/css" href="css/bootstrap-theme.css">
</head>





<body>

<!-- NAVBAR
	======================================================================================================================================= -->	   	
	    <div class="navbar-wrapper">
			<div class="container">
			
				<!-- static-top makes navbar not whole width of screen
					 fixed-top makes navbar the whole width of screen -->				 
				<nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
					<div class="container">
						<div class="navbar-header">
							<button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".navbar-collapse">
								<span class="sr-only">Toggle navigation</span>
								<span class="icon-bar"></span>
								<span class="icon-bar"></span>
								<span class="icon-bar"></span>
								
							</button>
							
							<!--BRAND-->
							<a class="navbar-brand" href="/login">Cogs121:A1</a><!-- a tag is used for linking-->		
											
						</div><!--close navbar-header-->
						
						<div class="navbar-collapse collapse">
							<form class="navbar-form navbar-right" role="form">														
								<button class="btn btn-danger" type="submit"> Logout </button>
									
	
								
							</form>							
	
						</div><!--close navbar-collapse-->					
						
					</div><!--close container w/in navbar-->	
				</nav>
			</div><!--close container-->
		</div><!--close navbar-wrapper-->

		<br>
		<br>


      <div class="container">
      	<h1 style="text-align:center"> Instagram Feed w/Caption </h1>
      	
      	
        <div class="row">
            
           
	            {{#each photos}}
	            
	              <div class="col-sm-12" class="images">
	              	<div class = "jumbotron">
		              	<center>
		                	<img src="{{url}}" class="img-responsive" class="img hashtag-img" width="80%">
		                
		                    <br>   
		                                            
							<p> {{caption}} </p>
		                
							<h2 style="color:blue"> {{likes}} likes </h2>
						</center>
	                                
					</div>
	              </div>
	              
	            {{/each}}

            
         </div>
         
      </div>
      <br>
      
    <script src="https://code.jquery.com/jquery.js"></script>
    <script src="js/jquery.validate.min.js"></script>
    <script src="js/additional-methods.min.js"></script>
    <script src="js/validate.js"></script>
    <script src="js/bootstrap.js"></script>
</body>
</html>



